{
  "name": "veramo-agent-deploy",
  "version": "1.0.0",
  "description": "General purpose Veramo Agent configuration deployments",
  "repository": "https://github.com/uport-project/veramo-agent-deploy",
  "author": "jason.healy@mesh.xyz",
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_ENV=production yarn veramo server --config=./config/agent.yml",
    "dev": "yarn veramo server --config=./config/agent-local.yml",
    "prettier": "prettier --write '{src, !build}/**/*.{ts,js,json,md,yml}'",
    "register-commands": "rm -rf ./build && yarn build && node ./build/discord/register-commands",
    "build": "tsc",
    "build:watch": "tsc -b --watch"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@veramo/cli": "3.0.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node-fetch": "2.5.12",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
